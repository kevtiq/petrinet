{
  "name": "petrinet",
  "version": "1.0.0",
  "description": "js petrinet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib dist es tsconfig.tsbuildinfo",
    "test:watch": "npm test -- --watchAll --coverage",
    "build": "npm run clean && tsc && tsc -b ./tsconfig.json",
    "prepublish": "npm run build",
    "bundlesize": "bundlesize --config bundlesize.config.json",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "npm-run-all --parallel lint test build && npm run bundlesize"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevtiq/petrinet.git"
  },
  "keywords": [],
  "author": "Kevin Pennekamp <github@kevtiq.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevtiq/petrinet/issues"
  },
  "homepage": "https://github.com/kevtiq/petrinet#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "bundlesize": "^0.18.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.4"
  },
  "dependencies": {}
}
